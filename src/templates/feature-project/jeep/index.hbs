{{#extend "layout"}}

  {{#replace "meta"}}
    <title>Hannah Sutton Design</title>
    <meta name="description" content="Hannah Sutton Design website">
  {{/replace}}

  {{#replace "body-id"}}project-jeep{{/replace}}

  {{#replace "main"}}

    <section id="hero">
      <div>
        <img src="/assets/img/portfolio/jeep/hero.jpg">
      </div>
    </section>

    <section id="details">
      <div>
        <img src="/assets/img/icons/line-down_animated.svg">
        <h2>Happy 75th Birthday, Jeep! <br>We’ve wrapped you a very <br>special present.</h2>
        <img src="/assets/img/icons/line-down-dot-short-animated.svg">
        <h3>To celebrate Jeep’s 75th Anniversary, Aaron <br>Holder of Asylum Graphics was tasked with <br>installing a series of promotional wraps <br>for Melbourne City Jeep. After 80 hours of <br>drawing, Hannah completed a mural for <br>every panel of the car which celebrated <br>Jeep’s influence on Australia over 75 years.</h3>
        <h3>Artwork inspired by Drawing Book’s original <br>concept for the ‘Etched In Mud’ Campaign</h3>
        <h3>Art Direction &amp; Signage by Asylum Graphics</h3>
        <img src="/assets/img/icons/small-arrow-down-animated.svg">
      </div>
    </section>

    <section id="image-display">
      <div>
        <div>
          <img src="/assets/img/portfolio/jeep/boot@2x.jpg">
          <img src="/assets/img/portfolio/jeep/close@2x.jpg">
        </div>
        <img src="/assets/img/portfolio/jeep/bonnet@2x.jpg">
      </div>
    </section>

    <section id="banner">
      <div>
        <h2>‘Our history is etched in mud.’ <br>- Jeep</h2>
      </div>
    </section>

    <section id="hero">
      <div>
        <img src="/assets/img/portfolio/jeep/flat-artwork.jpg">
      </div>
    </section>

    {{> portfolio-footer}}

  {{/replace}}
{{/extend}}
