      <div>
        <h2>Services</h2>
        <img src="/assets/img/services/egg.svg">
        <img src="/assets/img/services/bracket-design.svg">
      </div>
    </section>

    <section id="graphic-design">
      <div>
        <h2>Graphic design</h2>
        <img src="/assets/img/services/bracket-design-middle.svg">
        <div>
          <ul>
            <h4>Branding</h4>
            <img src="/assets/img/services/arrow-down.svg">
            <li>Logo</li>
            <li>Style Guide</li>
            <li>Advertising</li>
          </ul>
          <ul>
            <h4>Digital</h4>
            <img src="/assets/img/services/arrow-down.svg">
            <li>UI/UX Design</li>
            <li>App Design</li>
            <li>Social Media</li>
          </ul>
          <ul>
            <h4>Print</h4>
            <img src="/assets/img/services/arrow-down.svg">
            <li>Stationery</li>
            <li>Brochures</li>
            <li>Packaging</li>
            <li>Signage</li>
          </ul>
        </div>
      </div>
    </section>

    <section id="websites">
      <div>
        <img src="">
        <h2>Websites</h2>
        <img src="/assets/img/services/bracket-web.svg">
